<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>除了系统自带App，其他App在后台访问照片将被阻止，可能导致App在后台作恶时直接挂掉</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.twotrees.nobackgroundphotoaccessprefer</string>
			<key>key</key>
			<string>Enabled</string>
			<key>label</key>
			<string>总开关</string>
		</dict>		
		<dict>
			<key>label</key>
			<string>Github:</string>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>https://github.com/twotreeszf/NoBackgroundPhotoAccess</string>
		</dict>
	</array>
	<key>title</key>
	<string>禁止 App 在后台访问照片</string>	
</dict>
</plist>
