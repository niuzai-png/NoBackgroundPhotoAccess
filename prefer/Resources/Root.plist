<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Prevent  apps accessing photos in the background except system apps, feel free to take it, and I lose if you steal a photo backstage.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.twotrees.nobackgroundphotoaccessprefer</string>
			<key>key</key>
			<string>Enabled</string>
			<key>label</key>
			<string>Enable</string>
		</dict>		
		<dict>
			<key>label</key>
			<string>Github:</string>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>https://github.com/twotreeszf/NoBackgroundPhotoAccess</string>
		</dict>
	</array>
	<key>title</key>
	<string>NoBackgroundPhotoAccess</string>	
</dict>
</plist>
